			"objectValue": [
				{
					"data1": "User 1",
					"data2": 1,
					"data3": null
				},
				{
					"data1": "User 2",
					"data2": 2,
					"data3": null
				}
			]
		},
		{
			"id": "product1",
			"class": "net.mcfarb.testing.ddmock.sample.SampleData",
			"objectValue": {
				"data1": "Product ABC",
